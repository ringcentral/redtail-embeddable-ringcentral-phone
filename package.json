{
  "name": "redtail-embeddable-ringcentral-phone",
  "version": "2.2.1",
  "description": "RingCentral Embeddable Voice with Chrome extension for RedtailCRM",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress -w",
    "start": "npm run build",
    "postinstall": "node bin/post-install",
    "lint": "./node_modules/.bin/standard",
    "fix": "./node_modules/.bin/standard --fix",
    "docs": "mkdocs serve",
    "docs-b": "mkdocs build",
    "docs-p": "mkdocs gh-deploy",
    "deploy": "npm run docs-b && npm run docs-p",
    "down": "node bin/download-adapter.js",
    "pack": "rm -rf dist && cross-env NODE_ENV=production ./node_modules/.bin/webpack && npm run down && node bin/pack"
  },
  "keywords": [
    "RingCentral",
    "embeddable",
    "phone"
  ],
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "devDependencies": {
    "@ant-design/icons": "^4.0.6",
    "@babel/cli": "7.4.3",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "7.4.0",
    "@babel/runtime": "7.4.3",
    "antd": "^4.1.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "1.10.0",
    "babel-plugin-lodash": "3.3.4",
    "concat-loader": "0.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "5.1.6",
    "css-loader": "^2.1.1",
    "dayjs": "1.7.7",
    "extract-loader": "2.0.1",
    "file-loader": "1.1.11",
    "happypack": "5.0.0",
    "html-loader": "0.5.5",
    "jquery": "^3.5.0",
    "json-deep-copy": "^1.1.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash-webpack-plugin": "0.11.5",
    "moment": "2.23.0",
    "npm-run-all": "4.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-rename-selector": "^0.1.1",
    "pug-html-loader": "1.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recursive-assign": "0.2.0",
    "replace-in-file": "^5.0.2",
    "ringcentral-embeddable-extension-common": "0.9.8",
    "shelljs": "0.8.2",
    "standard": "^12.0.1",
    "style-loader": "0.23.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "1.1.2",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1",
    "webpack-extraneous-file-cleanup-plugin": "2.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "chrome",
      "MutationObserver"
    ]
  },
  "dependencies": {},
  "bin": {
    "redtail-embeddable-ringcentral-phone": "post-install.js"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/redtail-embeddable-ringcentral-phone.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringcentral/redtail-embeddable-ringcentral-phone/issues"
  },
  "homepage": "https://github.com/ringcentral/redtail-embeddable-ringcentral-phone#readme"
}
